        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:MusicCollectionWPF.Infra"
        Style="{StaticResource WindowT}"
        SizeToContent="WidthAndHeight"
        Title="Multi Album Editor" 
        xmlns:my="clr-namespace:MusicCollectionWPF.UserControls">
    <local:CustomWindow.Resources>
        <ResourceDictionary Source="../Dictionary/RessourcesDictionary.xaml" />
        <!--<ResourceDictionary Source="../Infra/RessourcesDictionary.xaml" />-->
    </local:CustomWindow.Resources>
    <Grid Background="{StaticResource Background}" Height="250" Width="600">
        <my:MultiTrackEditor x:Name="multiTrackEditor1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</local:CustomWindow>
