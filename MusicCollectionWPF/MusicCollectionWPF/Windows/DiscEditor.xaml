<local2:CustomWindow x:Class="MusicCollectionWPF.Windows.DiscEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:MusicCollectionWPF.Windows"
        xmlns:y="clr-namespace:MusicCollectionWPF.UserControls"
        xmlns:local2="clr-namespace:MusicCollectionWPF.Infra"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterScreen"   
        Style="{StaticResource WindowT}" 
        BorderBrush="{StaticResource Foreground}"    
         SizeToContent="WidthAndHeight"
        Title="Disc Editor" KeyDown="RootGrid_KeyDown" Loaded="Window_Loaded" >
    <!--CornerRadius="10" BorderThickness="4" Icon="..\music2.ico"-->
    <Grid>
        <y:AlbumEditor x:Name="Editor"  />
    </Grid>
</local2:CustomWindow>
